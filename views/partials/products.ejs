<% for(var i=0; i < products.length; i++) {%>
    <div class="row">
        <% for(var j=0; j < products[i][j].length; j++) {%>
            <div class="col-sm-6 col-md-4">
                <div class="thumbnail">
                    <img src="<%= products[i][j].imagePath %>" alt="..." class="img-responsive">
                    <div class="caption">
                        <h3><%= products[i][j].title %></h3>
                        <p class="description"><%= products[i][j].description %></p>
                        <div class="clearfix">
                            <div class="price pull-left">$<%= products[i][j].price %></div>
                            <a href="/add-to-cart/<%= products[i][j]._id %>" class="btn btn-success pull-right" role="button">Add to Shopping Cart</a>
                        </div>
                    </div>
                </div>
            </div>
        <% } %>
    </div>
<% } %>
